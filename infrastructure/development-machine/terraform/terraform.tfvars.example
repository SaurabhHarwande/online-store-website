# Copy this file to terraform.tfvars and fill in your values

# Utho Cloud Configuration
utho_api_key = "your-utho-api-key-here"

# Instance Configuration
instance_name   = "development-machine"
instance_dcslug = "innoida" # Options: innoida, inmumbaizone2, etc.
instance_image  = "ubuntu-22.04-x64"
instance_planid = "10001" # Check Utho dashboard for available plan IDs

# GitHub Configuration (for Ansible provisioning)
github_token    = "ghp_your-github-token-here"  # Create at: GitHub → Settings → Developer settings → Personal access tokens
git_user_name   = "Your Name"
git_user_email  = "your.email@example.com"